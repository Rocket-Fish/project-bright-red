{
  "name": "xiv-queue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && npm run build:client && npm run build:server",
    "build:client": "cross-env VUE_APP_BACKEND_URL=http://localhost:3333 vue-cli-service build --dest dist/client",
    "build:server": "cross-env SSR=1 VUE_APP_BACKEND_URL=http://localhost:3333  vue-cli-service build --dest dist/server",
    "start": "node server.js",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.0.7",
    "axios": "^0.21.1",
    "bulma": "^0.9.2",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "luxon": "^1.26.0",
    "path": "^0.12.7",
    "serialize-javascript": "^5.0.1",
    "unique-names-generator": "^4.4.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/luxon": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
